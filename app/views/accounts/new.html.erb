

<!-- Side navigation -->
<div class="sidenav">
    <h1>New Account</h1>

    <ul> <p><form action="/login" method="get">
      <input type="submit" value="Login" 
           name="Submit" id="frm1_submit" />
  </form></p> <br /></ul>
       
  </div>
  
  <!-- Page content -->
  <div class="main">
                
      <%= form_for :account, url: '/accounts' do |f| %>
      Username: <%= f.text_field :username %> <br /> <br>
      Password: <%= f.password_field :password %> <br /><br>
      Password Confirmation: <%= f.password_field :password_confirmation %> <br /><br>
      <%= f.submit "Submit" %>
    <% end %> <br /><br>
        

</div>
