<h1><%= @account.username %></h1>

<p>   groups owned: </p> 
    
    <% @all_owned_groups.each do |g| %>
    <%= link_to g.name, group_path(g) %> <br>
    <% end %>

    <p>   group membership: </p> 

    <% @account.groups.each do |g| %>
    <%= link_to g.name, group_path(g) %> <br/>
    <% end %> <br>

    <%= button_to "Back to plans", plans_path , :method => :get %> <br />
    <%= button_to "Log out", '/logout' , :method => :post %>